{
  "name": "geargrab",
  "version": "5.0.1",
  "private": true,
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:optimized": "NODE_ENV=production vite build --mode production",
    "preview": "vite preview",
    "start": "node build/index.js",
    "analyze": "npx vite-bundle-analyzer",
    "lighthouse": "npx lighthouse https://geargrab.co --output=html --output-path=./lighthouse-report.html",
    "test": "node scripts/alternating-test-runner.js",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:jest:coverage": "jest --coverage",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:cypress:headless": "cypress run --headless",
    "test:unit": "jest --testPathPatterns=tests/unit",
    "test:integration": "jest --testPathPatterns=tests/integration",
    "test:auth": "jest --testPathPatterns=tests/auth",
    "test:e2e": "cypress run --spec 'cypress/e2e/**/*.cy.{js,ts}'",
    "test:e2e:auth": "cypress run --spec 'cypress/e2e/auth/**/*.cy.{js,ts}'",
    "test:e2e:core": "cypress run --spec 'cypress/e2e/{homepage,browse,listing-details,list-gear}.cy.js'",
    "test:e2e:dashboard": "cypress run --spec 'cypress/e2e/dashboard.cy.js'",
    "test:e2e:payment": "cypress run --spec 'cypress/e2e/payment-flow.cy.js'",
    "test:e2e:mobile": "cypress run --spec 'cypress/e2e/mobile-functionality.cy.{js,ts}'",
    "test:all": "npm run test:jest && npm run test:cypress",
    "test:alternating": "node scripts/alternating-test-runner.js",
    "test:quick": "node scripts/quick-test-runner.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.0.0",
    "@sveltejs/kit": "^1.30.4",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/supertest": "^6.0.3",
    "autoprefixer": "^10.4.14",
    "bcrypt": "^6.0.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jest": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "postcss": "^8.4.24",
    "supertest": "^7.1.1",
    "svelte": "^4.2.20",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.4.0",
    "typescript": "^5.0.0",
    "vite": "^4.5.14"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.8",
    "@stripe/stripe-js": "^7.3.1",
    "@types/socket.io": "^3.0.1",
    "@types/socket.io-client": "^1.4.36",
    "@types/web-push": "^3.6.4",
    "compression": "^1.8.0",
    "date-fns": "^4.1.0",
    "firebase": "^10.14.1",
    "firebase-admin": "^12.6.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^18.2.0",
    "terser": "^5.43.1",
    "web-push": "^3.6.7",
    "zod": "^3.25.67"
  }
}
