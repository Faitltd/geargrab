{
  "name": "geargrab",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node build/index.js",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "cleanup": "node scripts/cleanup-code.js",
    "fix:imports": "node scripts/fix-imports.js",
    "quality:check": "npm run lint && npm run check && npm run cleanup",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test": "cypress run",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:unit:ci": "jest --ci --coverage --watchAll=false",
    "test:auth:unit": "jest --testPathPattern=auth",
    "test:auth:e2e": "cypress run --spec \"cypress/e2e/auth/**/*.cy.js\"",
    "test:auth:e2e:simple": "cypress run --config-file cypress-simple.config.js --spec \"cypress/e2e/auth/ultra-simple.cy.js\"",
    "test:auth:all": "npm run test:auth:unit && npm run test:auth:e2e:simple",
    "test:auth:watch": "jest --testPathPattern=auth --watch",
    "test:auth:continuous": "node scripts/continuous-test-runner.js",
    "test:auth:sequential": "node scripts/sequential-test-runner.js",
    "test:auth:concurrent": "concurrently \"npm run test:auth:watch\" \"npm run dev\" \"npm run test:auth:continuous\"",
    "test:transaction:flow": "node scripts/transaction-flow-test-runner.js",
    "test:comprehensive": "node scripts/geargrab-comprehensive-test-suite.js",
    "test:gcts": "node scripts/geargrab-comprehensive-test-suite.js",
    "test:integration:jest": "jest --testPathPattern=integration",
    "test:all:jest": "npm run test:unit && npm run test",
    "security:test": "node scripts/security-test.js",
    "docs:api": "node scripts/generate-api-docs.js all",
    "docs:api:validate": "node scripts/generate-api-docs.js validate",
    "docs:api:interactive": "node scripts/generate-api-docs.js interactive",
    "docs:api:postman": "node scripts/generate-api-docs.js postman",
    "docs:api:serve": "cd docs/api && python3 -m http.server 8080",
    "security:audit": "npm audit && npm audit fix",
    "lint:security": "npm audit --audit-level moderate",
    "build:secure": "npm run security:audit && npm run build",
    "test:smoke": "node cypress/scripts/run-tests.js smoke",
    "test:core": "node cypress/scripts/run-tests.js core",
    "test:auth": "node cypress/scripts/run-tests.js auth",
    "test:dashboard": "node cypress/scripts/run-tests.js dashboard",
    "test:responsive": "node cypress/scripts/run-tests.js responsive",
    "test:integration:cypress": "node cypress/scripts/run-tests.js integration",
    "test:all:cypress": "node cypress/scripts/run-tests.js all",
    "test:parallel": "node cypress/scripts/run-tests.js parallel core auth dashboard",
    "test:ci": "node cypress/scripts/run-tests.js ci",
    "test:setup": "node cypress/scripts/setup-tests.js",
    "setup:firebase": "node scripts/setup-firebase-env.js",
    "test:production-readiness": "node scripts/production-readiness-test.js",
    "audit:security": "node scripts/security-audit.js",
    "deploy:check": "npm run test:production-readiness && npm run audit:security",
    "test:fcra": "node -e \"import('./src/lib/jobs/finalAdverseCron.js').then(m => m.finalAdverseNoticeJob.runJob())\"",
    "test:background-check": "curl -X POST http://localhost:5173/api/webhooks/iprospect?reportId=test_report_123",
    "test:iprospectcheck": "node scripts/test-iprospectcheck.js",
    "verify-google-auth": "node scripts/verify-google-auth.js"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@cypress/webpack-preprocessor": "^6.0.4",
    "@eslint/js": "^9.29.0",
    "@jest/globals": "^30.0.2",
    "@sveltejs/adapter-node": "^1.0.0",
    "@sveltejs/kit": "^1.30.4",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cypress": "^0.1.6",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^10.0.0",
    "chokidar": "^4.0.3",
    "compression": "^1.8.0",
    "concurrently": "^9.1.2",
    "cypress": "^14.5.0",
    "eslint": "^9.29.0",
    "eslint-plugin-svelte": "^3.9.2",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "morgan": "^1.10.0",
    "postcss": "^8.4.24",
    "puppeteer": "^24.10.1",
    "supertest": "^7.1.1",
    "svelte": "^4.2.20",
    "svelte-check": "^3.4.3",
    "svelte-eslint-parser": "^1.2.0",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.5.14"
  },
  "dependencies": {
    "@stripe/stripe-js": "^7.3.1",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "express": "^4.21.1",
    "express-rate-limit": "^7.5.0",
    "firebase": "^10.14.1",
    "firebase-admin": "^12.6.0",
    "helmet": "^8.1.0",
    "isomorphic-dompurify": "^2.25.0",
    "joi": "^17.13.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.1.0",
    "nodemailer": "^7.0.3",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.4",
    "stripe": "^18.2.0",
    "svelte-dnd-action": "^0.9.61",
    "twilio": "^5.7.0"
  }
}